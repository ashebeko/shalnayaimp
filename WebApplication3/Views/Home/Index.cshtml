    <div class="vertical-center">
        <div class="container">
            <div class="row d-flex justify-content-center">
                <div class="col-md-12">
                    <div class="row">
                        <div class="col-lg-4">
                            <img style="padding-top: 55px; width: 57%; float: right" src="~/Content/Images/50cent.png" />
